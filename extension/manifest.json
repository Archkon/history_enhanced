{
  "manifest_version": 3,
  "name": "History Enhanced",
  "version": "1.0",
  "description": "An enhanced browser history viewer with better visualization and search capabilities",
  "permissions": [
    "history",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Group History by Domain"
  },
  "background": {
    "service_worker": "background.js"
  }
}
